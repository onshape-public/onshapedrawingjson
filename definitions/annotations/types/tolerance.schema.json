{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://drawing-schemas.onshape.com/annotations/types/tolerance",
	"title": "DimensionTolerance",
	"type": "object",
	"$comment": "Dimension tolerance display",
	"properties": {
		"type": {
			"type": "string",
			"enum": [
				"None",
				"Symmetrical",
				"Deviation",
				"Limits",
				"Basic",
				"MIN",
				"MAX",
				"Fit",
				"FitWithTolerance",
				"FitToleranceOnly"
			]
		}
	},
	"required": [
		"type"
	],
	"oneOf": [
		{
			"title": "Deviation / Symmetrical / Limits",
			"properties": {
				"upperValue": {
					"description": "Upper value of tolerance, if tolerance type is Deviation / Symmetrical / Limits.",
					"type": "number"
				},
				"lowerValue": {
					"description": "Lower value of tolerance, if tolerance type is Deviation / Limits.",
					"type": "number"
				}
			},
			"required": [
				"upperValue"
			]
		},
		{
			"title": "Fit class tolerance",
			"properties": {
				"fitType": {
					"description": "Fit type/Fit with tolerance/Fit Tolerance only",
					"type": "string",
					"enum": [
					"Clearance",
					"Transition",
					"Interference",
					"Userdefined"
					]
				},
				"holeClass": {
					"description": "Hole class.",
					"type": "string"
				},
				"shaftClass": {
					"description": "Shaft class.",
					"type": "string"
				},
				"activeParameter": {
					"description": "Hole/Shaft class.",
					"type": "string",
					"enum": [
					"Hole",
					"Shaft"
					]
				},
				"precision": {
					"description": "Tolerance precision(Fit with tolerance/fit tolerance only) ",
					"type": "number"
				},
				"isPrecisionOverridden": {
					"description": "True if the tolerance precision is overridden.",
					"type": "boolean"
				}
			},
			"required": [
				"fitType",
				"holeClass",
				"shaftClass"
			]
		}
	]
}