{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://drawing-schemas.onshape.com/differentAPIs/request/createannotationrequest",
  "title": "Create annotations",
  "type": "object",
  "properties": {
    "formatVersion": {
      "type": "string",
      "description": "Schema version",
      "const": "2021-01-01"
    },
    "requestId": {
      "type": "string",
      "description": "Unique id for proper request tracking"
    },
    "messageName": {
      "type":" string",
      "const": "onshapeCreateAnnotations",
      "description": "Type of request"
    },
    "description": {
      "description": "History entry for the requested change in drawing",
      "title": "History content",
      "type": "string"
    },
    "annotations": {
      "description": "Annotations",
      "type": "array",
      "items": {
        "$ref": "../../annotation.schema.json"
      }
    }
  },
  "additionalProperties": false,
  "required": [ "messageName", "annotations" ],
  "oneOf": [ "formatVersion" ]
}
